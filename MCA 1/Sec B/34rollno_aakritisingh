//html file saved - navifationframe.html

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Frame</title>
</head>
    <frameset cols="40%,60%">
        <frame src="home1.html" scrolling="no">
        <frame src="" name="link" scrolling="no">
    </frameset>
</html>

